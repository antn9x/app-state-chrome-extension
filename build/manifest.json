{
    "manifest_version": 3,
    "name": "app-state-chrome-extension",
    "short_name" : "App-State",
    "description": "app state chrome extension",
    "version": "1.2",
    "devtools_page": "devtools.html",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "tabs",
        "http://*/*",
        "https://*/*"
    ],
    "icons": {
        "128": "icon128.png"
    }
}
